<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.csuf.cpsc411.cpsc411project">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:usesCleartextTraffic="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.CPSC411Project">
        <activity android:name=".RegisterUser" />
        <activity android:name=".Reports" />
        <activity android:name=".SalesHistory" />
        <activity android:name=".Inventory" />
        <activity android:name=".InventoryMenu" />
        <activity android:name=".InventoryAddItem" />
        <activity android:name=".MakeTransaction" />
        <activity android:name=".MainMenu" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>